{
  "name": "@saibotsivad/braidedjs",
  "version": "0.0.0",
  "description": " Enterprise-ready, cost effective, highly flexible API using AWS and Cloudflare.",
  "scripts": {
    "dev": "run-p dev:* dev:service:*",
    "dev:rollup": "rollup -c -w",
    "dev:service:api-rest": "cd service/api-rest && wrangler preview --watch",
    "dev:service:api-ws": "echo api-ws",
    "dev:service:stream-ws": "echo stream-ws",
    "dev:service:www-redirect": "echo www-redirect",
    "test": "node -r esm app/node_modules/api-rest/handle-request.test.js",
    "build": "rollup -c",
    "predeploy": "npm run build",
    "deploy": "run-s deploy:*",
    "deploy:api-rest": "source secrets.sh && cd service/api-rest && serverless deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saibotsivad/braidedjs.git"
  },
  "keywords": [
    "jsonapi",
    "cloudflare",
    "aws",
    "dynamodb",
    "websockets"
  ],
  "author": "saibotsivad",
  "bugs": {
    "url": "https://github.com/saibotsivad/braidedjs/issues"
  },
  "homepage": "https://braidedjs.com",
  "devDependencies": {
    "@cloudflare/wrangler": "^1.6.0",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "aws-sdk": "^2.616.0",
    "esm": "^3.2.25",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.29.1",
    "sade": "^1.7.0",
    "serverless": "^1.61.3",
    "serverless-cloudflare-workers": "^1.2.0",
    "sign-aws-requests": "^1.0.1",
    "zora": "^3.1.8"
  },
  "dependencies": {
    "got": "^10.5.5",
    "just-split": "^2.1.0",
    "p-queue": "^6.3.0",
    "trouter": "^3.1.0"
  }
}
